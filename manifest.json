{
    "manifest_version": 2,
    "name": "iEatBackground",
    "description": "Add a custom background",
    "version": "1.0",
    "icons": {
        "16": "icons/icon_16.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "page_action": {
        "default icon": {
            "16": "icons/icon_16.png",
            "48": "icons/icon_48.png",
            "128": "icons/icon_128.png"
        }
    },
    "content scripts": [{
        "matches":["https://www.google.com/*"],
        "css": ["main.css"]
    }]
}